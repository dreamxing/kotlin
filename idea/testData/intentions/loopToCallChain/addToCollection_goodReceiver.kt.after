import java.util.ArrayList

fun foo(list: List<MutableCollection<Int>>) {
    var target: MutableCollection<Int>()
    <caret>target = list.mapTo(ArrayList<Int>()) { it.size }

    if (target.size > 100) {
        target = ArrayList<Int>()
    }
}